{
    "family": "youssef-tsk-def-pipeline",
    "containerDefinitions": [
        {
            "name": "youssef-app",
            "image": "youssefajlani/simplewhale",
            "memory": "100",
            "cpu": "99",
            "portMappings": [
                {
                    "containerPort": 3000,
                    "hostPort": 3000,
                    "protocol": "tcp"
                }
            ]
        }
    ],
    "requiresCompatibilities": [
        "FARGATE"
    ],
    "networkMode": "awsvpc",
    "memory": "512",
    "cpu": "256"
}